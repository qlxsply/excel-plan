<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.avalon.excelplan.mapper.GoodsMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.avalon.excelplan.entity.po.Goods">
        <id column="id" property="id" />
        <result column="good_code" property="goodCode" />
        <result column="good_name" property="goodName" />
        <result column="sale_user" property="saleUser" />
        <result column="category_1" property="category1" />
        <result column="category_2" property="category2" />
        <result column="category_3" property="category3" />
        <result column="price" property="price" />
        <result column="quantity" property="quantity" />
        <result column="unit_code" property="unitCode" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, good_code, good_name, sale_user, category_1, category_2, category_3, price, quantity, unit_code
    </sql>

</mapper>
